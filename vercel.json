{
  "redirects": [
    {
      "source": "/vitrine-cultural",
      "destination": "/agenda",
      "permanent": true
    },
    {
      "source": "/destaques",
      "destination": "/agenda", 
      "permanent": true
    },
    {
      "source": "/eventos",
      "destination": "/agenda",
      "permanent": true
    },
    {
      "source": "/eventos/(.*)",
      "destination": "/evento/$1",
      "permanent": true
    },
    {
      "source": "/highlights",
      "destination": "/agenda",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}